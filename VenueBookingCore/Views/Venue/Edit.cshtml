@model VenueBookingCore.Models.Venue
<h2>Edit </h2>
<form asp-action="Edit" asp-Controller="Venue" method="post">
    <div class="form-group">
        <label asp-for="VenueId" class="form-control" col-sm-2">Id</label>
        <input asp-for="VenueId" class="form-control" col-sm-10 readonly="true" />
    </div>
    <div class="form-group">
        <label asp-for="EventName" class="form-control" col-sm-2">EventName</label>
        <input asp-for="EventName" class="form-control" col-sm-10 />
    </div>
    <div class="form-group">
        <label asp-for="VenueName" class="form-control" col-sm-2">VenueName</label>
        <input asp-for="VenueName" class="form-control" col-sm-10 />
    </div>
    <div class="form-group">
        <label asp-for="Price" class="form-control" col-sm-2"> Price</label>
        <input asp-for="Price" class="form-control" col-sm-10 />
    </div>
    <div class="form-group">
        <label asp-for="Address" class="form-control" col-sm-2">Address</label>
        <input asp-for="Address" class="form-control" col-sm-10 />
    </div>

    <div>
        <button type="submit" class="btb btn-success">Update</button>
    </div>
</form>